<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="OutLook.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:OutLook"
    xmlns:local2="clr-namespace:OutLook.Page"
    Shell.FlyoutBehavior="Disabled">

    <ShellContent
        Title="Home"
        ContentTemplate="{DataTemplate local:MainPage}"
        Route="MainPage" />

    <ShellContent
        Title="NextPage"
        ContentTemplate="{DataTemplate local2:NextPage}"
        Route="Meals" />

</Shell>
